digraph finite_state_machine {
    node [shape = doublecircle]; Bf1;
    node [shape = doublecircle]; Bf2;
    node [shape = point ]; Bi;

    node [shape = circle];
    Bi -> B1;
    B1 -> B2 [ label = "a" ];
    B1 -> Bf1 [ label = "b" ];
    B2 -> Bf2 [ label = "a" ];
    B2 -> Bf1 [ label = "b" ];
    Bf1 -> Bf1 [ label = "a" ];
    Bf1 -> B1 [ label = "b" ];
    Bf2 -> Bf2 [ label = "a,b" ];
}
